{
  "name": "ecl-ide-express",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www --ignore ./landing_zone/ --ignore ./workspaces/",
    "codemirror:clean": "rm -rf ./public/javascripts/codemirror/",
    "codemirror:copy": "npx cpy '**/*' '../../public/javascripts/codemirror' --cwd='./node_modules/codemirror' --parents",
    "codemirror": "npm run codemirror:clean && npm run codemirror:copy",
    "papaparse:clean": "rm -rf ./public/javascripts/papaparse/",
    "papaparse:copy": "npx cpy node_modules/papaparse/papaparse.min.js public/javascripts/papaparse/",
    "papaparse": "npm run papaparse:clean && npm run papaparse:copy",
    "lodash:clean": "rm -rf ./public/javascripts/lodash/",
    "lodash:copy": "npx cpy '**/*' '../../public/javascripts/lodash' --cwd='./node_modules/lodash' --parents",
    "lodash": "npm run lodash:clean && npm run lodash:copy",
    "hpccjs:clean": "rm -rf ./public/javascripts/hpcc-js/",
    "hpccjs:copy": "npx cpy '**/*' '../../public/javascripts/hpcc-js' --cwd='./node_modules/@hpcc-js' --parents",
    "hpccjs": "npm run hpccjs:clean && npm run hpccjs:copy",
    "sortablejs:clean": "rm -rf ./public/javascripts/sortablejs/",
    "sortablejs:copy": "npx cpy node_modules/sortablejs/modular/sortable.esm.js public/javascripts/sortablejs/",
    "sortablejs": "npm run sortablejs:clean && npm run sortablejs:copy",
    "clientdeps": "npm run codemirror && npm run papaparse && npm run lodash && npm run hpccjs && npm run sortablejs"
  },
  "dependencies": {
    "@hpcc-js/comms": "^2.34.0",
    "@hpcc-systems/hsqlc": "file:hsqlc-1.0.0.tgz",
    "archiver": "^4.0.2",
    "aws-sdk": "^2.812.0",
    "bcrypt": "^5.0.0",
    "codemirror": "^5.58.3",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-session-sequelize": "^6.1.1",
    "cookie-parser": "^1.4.5",
    "cpy-cli": "^3.1.1",
    "csurf": "^1.11.0",
    "debug": "~4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "express-validator": "^6.8.0",
    "fs-extra": "^7.0.1",
    "get-stream": "^5.2.0",
    "http-errors": "^1.7.3",
    "morgan": "~1.9.0",
    "multer": "^1.4.2",
    "mysql2": "^1.7.0",
    "nodemailer": "^6.4.17",
    "papaparse": "^5.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "2.0.3",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "sequelize": "^5.22.3",
    "sortablejs": "^1.12.0",
    "yauzl": "^2.10.0"
  }
}
