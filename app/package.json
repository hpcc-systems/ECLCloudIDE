{
  "name": "ecl-ide-express",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www --ignore ./landing_zone/ --ignore ./workspaces/",
    "codemirror:clean": "rm -rf ./public/javascripts/codemirror/",
    "codemirror:copy": "npx cpx \"node_modules/codemirror/**/*\" public/javascripts/codemirror/",
    "codemirror": "npm run codemirror:clean && npm run codemirror:copy",
    "linenav:clean": "rm -rf ./public/javascripts/line-navigator/",
    "linenav:copy": "npx cpx \"node_modules/line-navigator/{file-wrapper.min.js,line-navigator.min.js}\" public/javascripts/line-navigator/",
    "linenav": "npm run linenav:clean && npm run linenav:copy",
    "lodash:clean": "rm -rf ./public/javascripts/lodash/",
    "lodash:copy": "npx cpx \"node_modules/lodash/**/*\" public/javascripts/lodash/",
    "lodash": "npm run lodash:clean && npm run lodash:copy",
    "clientdeps": "npm run codemirror && npm run linenav && npm run lodash"
  },
  "dependencies": {
    "aws-sdk": "^2.504.0",
    "bcrypt": "^3.0.3",
    "codemirror": "^5.44.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-session-sequelize": "^6.0.0",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.10.0",
    "debug": "~4.1.1",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "fs-extra": "^7.0.1",
    "http-errors": "~1.7.1",
    "line-navigator": "^2.1.6",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "mysql2": "^1.6.4",
    "nodemailer": "^6.2.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "2.0.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sequelize": "^5.21.1"
  },
  "devDependencies": {
    "cpx": "^1.5.0"
  }
}
